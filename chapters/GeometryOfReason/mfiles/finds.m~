function y=finds(s)
q=[0.50876,0.13934,0.35190];
r=[1/3,1/3,1/3];
rs=.70121;
y(1)=s(1)+s(2)+s(3)-1;
y(2)=(r(1)-s(1))^2+(r(2)-s(2))^2+(r(3)-s(3))^2-(rs^2);
y(3)=(q(1)-s(1))^2+(q(2)-s(2))^2+(q(3)-s(3))^2-((q(1)-r(1))^2+(q(2)-r(2))^2+(q(3)-r(3))^2);
endfunction
## [s, info] = fsolve (@stefsolve, [.3;.3;.4])
