x=[.1,.2,.7]
z=[(1/3)*(1-((x(1)-(1/3))/(x(1)-(1/3)))),(1/3)*(1-((x(2)-(1/3))/(x(1)-(1/3)))),(1/3)*(1-((x(3)-(1/3))/(x(1)-(1/3))))]
t=0.01:0.01:0.99;
plot(t,(((z(1).-x(1)).^2).*((x(1))./(((1.-t).*x(1)).+(t.*z(1))))).+(((z(2).-x(2)).^2).*((x(2))./(((1.-t).*x(2)).+(t.*z(2))))).+(((z(3).-x(3)).^2).*((x(3))./(((1.-t).*x(3)).+(t.*z(3))))));
