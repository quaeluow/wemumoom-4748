mm=.23;
m=[1/3;1/3;1/3];
z=[1/4;1/2;3/5];
for i = 1:99
z(1)=unifrnd(0,1);
z(2)=unifrnd(0,1);
z(3)=unifrnd(0,1);
min=(z(1)-m(1))*log((((1-mm)*m(1))+(mm*z(1)))/m(1))+(z(2)-m(2))*log((((1-mm)*m(2))+(mm*z(2)))/m(2))+(z(3)-m(3))*log((((1-mm)*m(3))+(mm*z(3)))/m(3));
sub=mm*(((z(1)-m(1))^2)/(m(1)))+mm*(((z(2)-m(2))^2)/(m(2)))+mm*(((z(3)-m(3))^2)/(m(3)));
dif=mm*(min-sub)
endfor















