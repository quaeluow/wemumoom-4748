size(500pt,500pt);
real m=1;

real mg=.25;
real ml=-mg;
real mr=10+mg;
real mu=10+mg;
real md=-mg;
path marg=((ml*m,mu*m)--(mr*m,mu*m)--(mr*m,md*m)--(ml*m,md*m)--cycle);
draw(marg,linewidth(0.5));

real frm=mg+1;
real fl=-frm;
real fr=10+frm;
real fu=10+frm;
real fd=-frm;
path frame=((fl*m,fu*m)--(fr*m,fu*m)--(fr*m,fd*m)--(fl*m,fd*m)--cycle);
draw(frame,linewidth(0.5));

